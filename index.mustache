{{> header_navbar }}

    <!-- Header -->
    <header>
        <div class="container" id="maincontent" tabindex="-1">
            <div class="row">
                <div class="col-lg-12">
                    <img class="img-responsive" src="img/celsiusnotes_256.png" alt="">
                    <div class="intro-text">
                        <h1 class="name">Kelvin Williams</h1>
                        <hr class="no-star">
                        <span class="skills">Software Engineer - Mobile and Backend</span>
                    </div>
                </div>
            </div>
        </div>
    </header>

    <!-- Portfolio Grid Section -->
    <section id="portfolio">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2>Portfolio</h2>
                    <hr class="no-star">
                </div>
            </div>
            <div class="row">
				{{#portfolio_list}}
                <div class="col-sm-4 portfolio-item">
                    <a href="{{link}}" class="portfolio-link" data-toggle="modal">
                        <label>{{timeframe}}</label>
                        <img src="{{img}}" class="img-responsive" alt="{{alt}}">
                        <label>{{title}}</label>
                    </a>
                </div>
				{{/portfolio_list}}
            </div>
        </div>
    </section>

    <!-- Resume Timeline Section https://webdesign.tutsplus.com/tutorials/building-a-vertical-timeline-with-css-and-a-touch-of-javascript--cms-26528 -->
	<section id="resume" class="timeline">
		<div class="row">
			<div class="col-lg-12 text-center">
				<h2>Resume</h2>
				<hr class="no-star">
			</div>
        </div>
  		<ul>
            {{#resume_list}}  
            <li>
                <div>
                    <time>{{timeframe}}</time> 
                    <label>{{title}}</label> <br>
                    {{description}}
                </div>
			</li>
            {{/resume_list}}
		</ul>
	</section>

	<!-- Skills Section -->
    <section id="skills">
		<div class="col-lg-12 text-center">
			<h2>Skills and Interests</h2>
			<hr class="no-star">
		</div>
		<div class="row">
			<div class="col-sm-4 skillcategory">
				<h3>Programming Languages</h3>
				<div class="row">
					<p class="col-md-6" >Swift<p>
					<p class="col-md-6" >Objective-C<p>
					<p class="col-md-6">JavaScript/ES6</p>
					<p class="col-md-6">HTML5/CSS3</p>
					<p class="col-md-6">Java</p>
					<p class="col-md-6">Python</p>
					<p class="col-md-6">Bash</p>
					<p class="col-md-6">SQL</p>
					<p class="col-md-6">C++</p>
					<p class="col-md-6">C</p>
				</div>
        	</div>

			<div class="col-sm-4 skillcategory">
				<h3>Technologies and Platforms</h3>
				<div class="row">
					<p class="col-md-6" >iOS<p>
					<p class="col-md-6" >React Native<p>
					<p class="col-md-6">Node.js/Express</p>
					<p class="col-md-6">Nginx</p>
					<p class="col-md-6">Docker</p>
					<p class="col-md-6">*nix Systems</p>
				</div>
        	</div>

			<div class="col-sm-4 skillcategory">
				<h3>Programming Languages</h3>
				<div class="row">
					<p class="col-md-6" >Swift<p>
					<p class="col-md-6" >Objective-C<p>
					<p class="col-md-6">JavaScript/ES6</p>
					<p class="col-md-6">HTML5 / CSS3</p>
					<p class="col-md-6">Java</p>
					<p class="col-md-6">Python</p>
					<p class="col-md-6">Bash</p>
					<p class="col-md-6">SQL</p>
					<p class="col-md-6">C++</p>
					<p class="col-md-6">C</p>
				</div>
        	</div>
		</div>
    </section>

    <!-- About Section -->
    <section class="success" id="about">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2>About</h2>
                    <hr class="no-star">
                </div>
            </div>
            <div class="row">
                <div class="col-lg-4 col-lg-offset-2">
                    <p>Hi, I am Kelvin. I am a Software Engineer with a Bachelors degree in Electrical Engineering and a Masters Degree in Computer Engineering. Currently I work as full stack mobile developer (native iOS and React Native) in Berlin. </p>
                </div>
				<div class="col-lg-4">
                    <p>This site serves as my online resume and portfolio. Head over to my developer blog on <a href="http://celsiusnotes.com"> celsiusnotes.com </a> if you like.</p>
                </div>
				
            </div>
        </div>
    </section>


 {{> contact_footer }}